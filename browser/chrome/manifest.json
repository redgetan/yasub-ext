{
  "name": "Yasub - Let's Subtitle",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.smartmediarep.com/*"
  ],
  "background": {
    "persistent": true,
    "scripts": ["jquery.js","background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://tvcast.naver.com/*"],
      "css": ["extension.css"],
      "js": [
        "jquery.js",
        "screenfull.js",
        "popcorn-complete.js",
        "popcorn_naver.js",
        "init_naver.js"
      ]
    }, 
    {
      "matches": ["*://tvcast.naver.com/*"],
      "js": [
        "inject_scripts_to_page.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {                    // optional
      "19": "erlang19.png",           // optional
      "38": "erlang38.png"            // optional
    },
    "default_title": "Yasub Translations",      // optional; shown in tooltip
    "default_popup": "popup.html"        // optional
  },
  "web_accessible_resources": ["232.png", "override_naver_callback.js"]
                                
}